{
  "name": "hexagonal",
  "version": "1.0.0",
  "description": "estructrura de arquitectura hexagonal en tyscript",
  "main": "main.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/main.ts",
    "start": "node dist/main.js",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "migrate:undo:all": "sequelize db:migrate:undo:all",
    "migrate:status": "sequelize db:migrate:status",
    "migrate:create": "sequelize migration:create --name",
    "migrate  :create:all": "sequelize migration:create --name all",
    "test": "npm run dev"
  },
  "author": "Geovanny Chavez",
  "license": "ISC",
  "dependencies": {
    "@types/yup": "^0.32.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "mysql2": "^3.14.1",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "sequelize-cli": "^6.6.3"
  }
}
